{
  "name": "legit-github-anomaly-detector",
  "version": "1.0.0",
  "description": "GitHub webhook-based anomaly detection system",
  "main": "dist/Main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/Main.js",
    "dev": "dotenv -e .env -- ts-node src/Main.ts",
    "watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "github",
    "webhook",
    "anomaly-detection"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@octokit/webhooks": "^14.2.0",
    "@octokit/webhooks-types": "^7.6.1",
    "octokit": "^5.0.5",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "smee-client": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/winston": "^2.4.4",
    "dotenv-cli": "^11.0.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
